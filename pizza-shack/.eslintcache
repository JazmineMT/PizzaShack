m>\r\n                    )\r\n                }else{\r\n\r\n                    return (\r\n                        <Item>\r\n                            <IMGBox>\r\n                                <IMG src={item.picURL}/>\r\n                            </IMGBox>\r\n                            <MiddleBox>\r\n                            <NameBox>\r\n                            <Title>{item.name}</Title>\r\n                            </NameBox>\r\n                            <DesBox>\r\n                            <Para>{item.description}</Para>\r\n                            </DesBox>\r\n                            </MiddleBox>\r\n                            <PriceBox>\r\n                                <Price > ${item.price.toFixed(2)}</Price>\r\n                                <Remove onClick={() =>{\r\n                                    props.remove(item)\r\n                                    sum = total - item.price\r\n                                    SetTotal(sum)\r\n                                }}> Remove</Remove>\r\n\r\n                            </PriceBox>\r\n                        </Item>\r\n                    )\r\n                }\r\n            })}\r\n             \r\n            </ItemsContainer>\r\n                {props.order.length > 0 && (\r\n                \r\n                <Last>\r\n                    \r\n                <Buttons className='btnBox'>\r\n                    <button onClick={refreshPage} className='button'> Checkout</button>\r\n                </Buttons>\r\n                <Total>\r\n\r\n                <h3 > Total</h3>\r\n                <h4>$ {total.toFixed(2)}</h4>\r\n                </Total>\r\n            </Last>)}\r\n        </Container>\r\n        </div>\r\n\r\n    )\r\n}","C:\\Users\\kamal\\Desktop\\Coding-Projects\\PizzaShack\\pizza-shack\\src\\Components\\Pages\\Pizza.js",["298","299","300"],"import React , {useEffect, useRef}from 'react'\r\nimport { useHistory} from \"react-router-dom\";\r\nimport {pizzas}