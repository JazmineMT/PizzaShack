Total] = useState(0)\r\n    let sum = 0 \r\n    console.log(props.order)\r\n\r\n    useEffect(() => {\r\n        getTotal()\r\n    },[])\r\n\r\n    const getTotal= () => {\r\n        props.order.map( item => {\r\n            sum = sum + item.price\r\n        })\r\n        SetTotal(sum)\r\n \r\n\r\n        \r\n    }\r\n    const remove = (item) => {\r\n        \r\n        props.remove(item)\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <h1 className='pageTitle'> My Order </h1>\r\n        <Container>\r\n        {props.order.length === 0 && (\r\n                <h1 className='empty'> There's nothing in your cart! </h1>\r\n            ) }\r\n            <ItemsContainer>\r\n\r\n            {props.order.map( item => {\r\n                if(item.name === 'specail'){\r\n                    return (\r\n                        \r\n                        <Item>\r\n                        <IMGBox>\r\n                            <IMG src={pizza}/>\r\n                        </IMGBox>\r\n                        <MiddleBox>\r\n                        <NameBox>\r\n                        <Title>{item.size} Pizza</Title>\r\n                        </NameBox>\r\n                        <DesBox>\r\n                        <Para>{item.size} {item.crust} crust Pizza, with {item.sauce} sauce,</Para>\r\n                        {item.toppings.map(topping => {    \r\n                            return(\r\n                                <div>\r\n                                    <Para>{topping}</Para>\r\n                                </div>\r\n                            )\r\n                        })}\r\n                        \r\n                        \r\n                        </DesBox>\r\n                        </MiddleBox>\r\n                        <PriceBox>\r\n                            <Price > ${item.price.toFixed(2)}</Price>\r\n                            <Remove onClick={() =>{\r\n                                    props.removeSpecail(item)\r\n       